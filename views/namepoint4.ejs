<% include partials/head.ejs %>
<% include partials/header.ejs %>
<main role="main">
	<section>
        <h1><%= title %></h1>
        <p><%= description %></p>
        <% results.forEach(result => { %> 
            <div class="results">
            <h2><%= result.name %></h2>
            <ul>
                <li>Surhed: <%= result.flavour_sourness %></li>
                <li>Styrke: <%= result.flavour_strenght %></li>
                <li>Type: <%= result.flavour_type %></li>
            </ul>
            </div>
        <% }); %>
	</section>
</main>
<% include partials/footer.ejs %>
<% include partials/after-footer.ejs %>